<%= form_tag(programs_path, method: :get, id: "search-form", class: "search-form navbar-right") do %>
  <div class="form-group">
    <%= text_field_tag :search, params[:search], placeholder: "Search programs", class:"form-control" %>
  </div>
  <%= submit_tag "Search", class: "btn btn-primary" %>
<% end %>